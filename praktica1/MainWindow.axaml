<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="praktica1.MainWindow"
        Title="praktica1">
	<DockPanel>
		<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
			<StackPanel Width="200">
				<TextBlock Text="Логин" HorizontalAlignment="Center">

				</TextBlock>
				<TextBox x:Name="log"></TextBox>
				<StackPanel
					Height="10">
				</StackPanel>
				<TextBlock Text="Пароль" HorizontalAlignment="Center">
				</TextBlock>
				<TextBox x:Name="pass"></TextBox>
			</StackPanel>
			<StackPanel
				Height="10">
			</StackPanel>
			<StackPanel>
				<Button Click="Login_click">
					Войти
				</Button>
			</StackPanel>
		</StackPanel>
	</DockPanel>
</Window>
